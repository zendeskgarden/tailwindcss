{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "prebuild": "tsc",
    "build": "vite build",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,.cjs,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint . --ext js,ts,tsx",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "vite --open",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "focus-visible": "5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@vitejs/plugin-react-swc": "3.0.1",
    "@zendeskgarden/eslint-config": "29.0.0",
    "@zendeskgarden/scripts": "1.4.0",
    "@zendeskgarden/svg-icons": "6.33.0",
    "@zendeskgarden/tailwindcss": "3.0.1",
    "autoprefixer": "10.4.13",
    "envalid": "7.3.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "markdownlint": "0.27.0",
    "postcss": "8.4.21",
    "postcss-focus-visible": "7.1.0",
    "prettier": "2.8.2",
    "prettier-package-json": "2.8.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-svgr": "2.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
