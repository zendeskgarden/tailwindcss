{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "prebuild": "tsc",
    "build": "vite build",
    "format": "prettier-package-json --write && prettier --log-level warn '**/*.{ts,tsx,js,cjs,mjs,json,md}' '!build/**' --write",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint . --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "start": "vite --open",
    "test:ci": "npm run -- format --check && npm run lint"
  },
  "dependencies": {
    "@zendeskgarden/react-forms": "9.14.2",
    "@zendeskgarden/react-theming": "9.14.2",
    "@zendeskgarden/react-typography": "9.14.2",
    "@zendeskgarden/tailwindcss": "4.0.2",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.3.11"
  },
  "devDependencies": {
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react-swc": "4.2.3",
    "@zendeskgarden/eslint-config": "47.1.0",
    "@zendeskgarden/scripts": "2.5.3",
    "@zendeskgarden/svg-icons": "8.1.0",
    "@zendeskgarden/tailwindcss": "4.0.2",
    "autoprefixer": "10.4.24",
    "envalid": "8.1.1",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "markdownlint-cli": "0.47.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-package-json": "2.8.0",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-svgr": "4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
