{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint src/ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "react-scripts start",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/classnames": "2.3.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.17",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@zendeskgarden/scripts": "1.4.0",
    "@zendeskgarden/svg-icons": "6.30.2",
    "@zendeskgarden/tailwindcss": "3.0.1",
    "autoprefixer": "10.4.0",
    "classnames": "2.3.1",
    "envalid": "7.2.2",
    "focus-visible": "5.2.0",
    "markdownlint": "0.25.0",
    "path": "0.12.7",
    "postcss": "8.4.5",
    "postcss-focus-visible": "6.0.2",
    "prettier-package-json": "2.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "tailwindcss": "3.0.8",
    "typescript": "4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
