{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint src/ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "craco start",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "@craco/craco": "6.3.0",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@tailwindcss/postcss7-compat": "2.2.16",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.1",
    "@testing-library/user-event": "13.2.1",
    "@types/classnames": "2.3.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.19",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@zendeskgarden/scripts": "1.2.1",
    "@zendeskgarden/svg-icons": "6.30.2",
    "@zendeskgarden/tailwindcss": "2.0.0",
    "autoprefixer": "9.8.7",
    "classnames": "2.3.1",
    "envalid": "7.2.1",
    "focus-visible": "5.2.0",
    "markdownlint": "0.24.0",
    "path": "0.12.7",
    "postcss": "7.0.38",
    "postcss-focus-visible": "5.0.0",
    "prettier-package-json": "2.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
