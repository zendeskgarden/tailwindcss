{
  "private": true,
  "name": "example",
  "homepage": "./",
  "version": "0.1.0",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint src/ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "craco start",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "@craco/craco": "6.1.1",
    "@tailwindcss/postcss7-compat": "2.0.4",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.7.1",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.20",
    "@types/node": "12.20.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@zendeskgarden/scripts": "0.1.11",
    "@zendeskgarden/svg-icons": "6.28.0",
    "@zendeskgarden/tailwindcss": "2.0.0",
    "autoprefixer": "9.8.6",
    "classnames": "2.2.6",
    "envalid": "7.0.0",
    "focus-visible": "5.2.0",
    "markdownlint": "0.23.1",
    "path": "0.12.7",
    "postcss": "7.0.35",
    "postcss-focus-visible": "5.0.0",
    "prettier-package-json": "2.1.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
