{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "prebuild": "tsc",
    "build": "vite build",
    "format": "prettier-package-json --write && prettier --log-level warn '**/*.{ts,tsx,js,cjs,mjs,json,md}' '!build/**' --write",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint . --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "start": "vite --open",
    "test:ci": "npm run -- format --check && npm run lint"
  },
  "dependencies": {
    "@zendeskgarden/react-forms": "9.1.0",
    "@zendeskgarden/react-theming": "9.1.0",
    "@zendeskgarden/react-typography": "9.1.0",
    "@zendeskgarden/tailwindcss": "4.0.2",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "3.7.1",
    "@zendeskgarden/eslint-config": "44.0.1",
    "@zendeskgarden/scripts": "2.4.2",
    "@zendeskgarden/svg-icons": "7.3.0",
    "@zendeskgarden/tailwindcss": "4.0.2",
    "autoprefixer": "10.4.20",
    "envalid": "8.0.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "markdownlint-cli": "0.42.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-plugin-svgr": "4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
