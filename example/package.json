{
  "private": true,
  "name": "example",
  "homepage": "./",
  "version": "0.1.0",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint src/ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "craco start",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "@craco/craco": "5.6.4",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.6",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.14",
    "@types/node": "12.12.62",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@zendeskgarden/scripts": "0.1.11",
    "@zendeskgarden/svg-icons": "6.23.0",
    "@zendeskgarden/tailwindcss": "1.0.1",
    "autoprefixer": "9.8.6",
    "classnames": "2.2.6",
    "envalid": "6.0.2",
    "focus-visible": "5.1.0",
    "markdownlint": "0.21.0",
    "path": "0.12.7",
    "postcss-focus-visible": "5.0.0",
    "prettier-package-json": "2.1.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.3",
    "tailwindcss": "1.8.10",
    "typescript": "4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
