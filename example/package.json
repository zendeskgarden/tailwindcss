{
  "private": true,
  "name": "example",
  "version": "0.1.0",
  "homepage": "./",
  "scripts": {
    "prebuild": "tsc",
    "build": "vite build",
    "format": "prettier-package-json --write && prettier --loglevel warn '**/*.{ts,tsx,js,cjs,mjs,json,md}' '!build/**' --write",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint . --ext js,mjs,ts,tsx",
    "lint:md": "markdownlint README.md",
    "prepare": "yarn build",
    "start": "vite --open",
    "test:ci": "yarn format --check && yarn lint"
  },
  "dependencies": {
    "@zendeskgarden/tailwindcss": "3.1.1",
    "classnames": "2.3.2",
    "focus-visible": "5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.9",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@zendeskgarden/eslint-config": "34.0.0",
    "@zendeskgarden/scripts": "2.0.1",
    "@zendeskgarden/svg-icons": "6.33.0",
    "@zendeskgarden/tailwindcss": "3.1.1",
    "autoprefixer": "10.4.14",
    "envalid": "7.3.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "markdownlint-cli": "0.33.0",
    "postcss": "8.4.26",
    "postcss-focus-visible": "8.0.2",
    "prettier": "2.8.8",
    "prettier-package-json": "2.8.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.4",
    "vite-plugin-svgr": "3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
